				if(confirm("Are you sure?")){
					$.ajax({
						url: `/management/advertisement/delete/${id}`,
					})
					.done(function(response) {

						if(response.success){
							alert('Deleted Successful');
							location.reload();
						} else {
							alert(`Error! ${response.msg}`)
						}
					})
				}
			}

			function setElligibilityStatus(id, value){
				if(confirm("Are you sure to change eligibility status?")){
					$.ajax({
						type: 'post',
						url: `/management/applicants/elligibile-status`,
						data: {
							id: id,
							value: value
						}
					})
					.done(function(response) {

						if(response.success){
							alert(response.msg);
							if(value) {
								$(`#${id}eligibleValue`).html(value);
							} else {
								$(`#${id}eligibleValue`).html('Not Set Yet');
							}
						} else {
							alert(response.msg);
						}
					})
				}
